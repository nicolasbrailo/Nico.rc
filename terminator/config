[global_config]
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  suppress_multiple_term_dialog = True
[keybindings]
  close_term = <Primary><Shift><Alt>w
  cycle_next = <Control>Right
  cycle_prev = <Control>Left
  go_left = <Shift><Control>Left
  go_next = <Control><Alt>w
  go_prev = <Control><Alt>q
  go_right = <Shift><Control>Right
  new_tab = <Primary>t
  new_window = <Primary><Shift>t
  reset_clear = <Control>k
  split_horiz = <Control><Alt>h
  split_vert = <Control><Alt>v
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_image = None
    font = Inconsolata Medium 18
    scrollback_infinite = True
    show_titlebar = False
    use_system_font = False
